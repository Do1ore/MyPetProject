@model IEnumerable<MyPet.Areas.Identity.Data.MyPetUser>
@{
    ViewBag.Title = "Список пользователей";
    ViewBag.Secondary = "Для администратора";
}
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access level</a>
            </td>
            <td>
                <form method="post" asp-action="Delete">
                    <input type="hidden" name="id" value="@user.Id">
                    <button type="submit" class="btn btn-sm btn-danger">Delete</button>
                </form>
            </td>
        </tr>
    }
</table>